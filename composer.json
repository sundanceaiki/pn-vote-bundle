{
  "name": "sundanceaiki/pn-vote-bundle",
  "description": "Provides voting functionality for Symfony",
  "keywords": [
    "like",
    "dislike",
    "voting",
    "vote",
    "bundle"
  ],
  "homepage": "https://github.com/sundanceaiki/pn-vote-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Catalin Avacaritei",
      "email": "sundance.aiki@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
	"sundanceaiki/pn-vote-bundle":"1.0.0",
    "symfony/form": "^3.4",
    "symfony/framework-bundle": "^3.4",
    "symfony/security-bundle": "^3.4",
    "symfony/twig-bundle": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0",
    "symfony/phpunit-bridge": "^4.1"
  },
  "minimum-stability": "dev",
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "VoteBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VoteBundle\\": ""
    }
  },
  "scripts": {
    "test": "bin/phpunit --color=always -v --debug"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0-dev"
    }
  },
  "prefer-stable": true
}
